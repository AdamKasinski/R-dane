# EEG-5

# Poprawność w uwagowych

dane <- tab$ogolne.all.1
data.frame(x) -> xs
data.frame(dane, ys) -> qwe
qwe$dane[qwe$ys=="SAT"] -> sat
qwe$dane[qwe$ys=="noSAT"] -> nosat
t.test(sat,nosat)

# czas

d2 <- tab$ogolne.all.9
data.frame(d2, ys) -> qwe
qwe$d2[qwe$ys=="SAT"] -> sat2
qwe$d2[qwe$ys=="noSAT"] -> nosat2
t.test(sat2,nosat2)
